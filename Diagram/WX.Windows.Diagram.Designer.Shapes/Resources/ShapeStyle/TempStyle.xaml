    <!-- 业务流程样式 -->
    <Style x:Key="BusinessProcedureStyle" TargetType="{x:Type ds:Shape}" BasedOn="{StaticResource BusinessArchitechStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>

                        <Path Data="F1 M 130.5,85.5L 0.5,85.5L 26.5003,43L 0.5,0.499985L 130.5,0.499985L 156.5,43L 130.5,85.5 Z" Margin="0,0,0,0" Style="{StaticResource BusinessArchitechPathItemStyle}" Grid.ColumnSpan="3" Stretch="Fill" Grid.RowSpan="3" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Brush x:Key="FlowChartPathItemStroke">#FFD69436</Brush>
    <LinearGradientBrush x:Key="FlowChartPathItemBrush" StartPoint="0,0" EndPoint="0,1">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="#FAFBE9" Offset="0" />
            <GradientStop Color="Orange" Offset="1" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <Style x:Key="FlowChartPathItemStyle" TargetType="Path">
        <Setter Property="Fill" Value="#FFE9C0BE"/>
        <Setter Property="Stroke" Value="#000000"/>
        <Setter Property="StrokeThickness" Value="1"/>
        <Setter Property="StrokeLineJoin" Value="Round"/>
        <Setter Property="Stretch" Value="Fill"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>


    <Style x:Key="SVGBusinessShapeStyle" TargetType="{x:Type ds:Shape}">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
    </Style>

    <Style x:Key="FlowChartStyle" TargetType="{x:Type ds:Shape}">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
    </Style>

    <Style x:Key="FlowChartProcessStyle" TargetType="{x:Type ds:Shape}" BasedOn="{StaticResource FlowChartStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>
                        <Path Data="M 0,0 H 60 V40 H 0 Z" Style="{StaticResource FlowChartPathItemStyle}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="FlowChartDecisionStyle" TargetType="{x:Type ds:Shape}" BasedOn="{StaticResource FlowChartStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>
                        <Path Data="M 0,20 L 30 0 L 60,20 L 30,40 Z" Style="{StaticResource FlowChartPathItemStyle}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="FlowChartDocumentStyle" TargetType="{x:Type ds:Shape}" BasedOn="{StaticResource FlowChartStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>
                        <Path Data="M 0,0 H 60 V 40 C 30,30 30,50 0,40 Z" Style="{StaticResource FlowChartPathItemStyle}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="FlowChartDataStyle" TargetType="{x:Type ds:Shape}" BasedOn="{StaticResource FlowChartStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid>
                        <Path Data="M 10,0 L 60 0 L 50,40 L 0,40 Z" Style="{StaticResource FlowChartPathItemStyle}"/>
