                        <Path Data="M 10,20 A 20,20 0 1 1 50,20 A 20,20 0 1 1 10,20" Style="{StaticResource FlowChartPathItemStyle}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

